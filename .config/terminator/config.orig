[global_config]
  enabled_plugins = InactivityWatch, CustomCommandsMenu, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  focus = mouse
  inactive_color_offset = 0.708333333333
  scroll_tabbar = True
  suppress_multiple_term_dialog = True
  title_use_system_font = False
[keybindings]
  help = <Primary>F1
  zoom_in = <Primary>equal
  zoom_out = <Primary>minus
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    audible_bell = True
    backspace_binding = automatic
    copy_on_selection = True
    cursor_blink = False
    cursor_color = "#aaaaaa"
    font = Inconsolata Medium 14
    icon_bell = False
    show_titlebar = False
    use_system_font = False
